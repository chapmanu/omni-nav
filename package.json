  "bugs": {
    "url": "https://github.com/chapmanu/omni-nav/issues"
  },
  "homepage": "https://github.com/chapmanu/omni-nav#readme",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.4",
    "expect.js": "^0.3.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "mocha": "^3.4.2",
    "mocha-sinon": "^2.0.0",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "sinon": "^2.3.7",
